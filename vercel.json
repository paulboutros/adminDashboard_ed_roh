{
    "version": 2,
    "routes": [
      {
        "src": "/server/(.*)",  
        "dest": "/server/$1"    
      },
      {
        "src": "/api/(.*)",    
        "dest": "/server/api/$1"  
      },
       
      {
        "src": "/static/(.*)", 
        "dest": "/public/$1"
      },
      {
        "src": "/(.*)",        
        "dest": "/src/$1"
      }
    ]
  }
  